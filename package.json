{
  "name": "vue-swipe-actions",
  "description": "iOS style swipe actions",
  "version": "1.0.12",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "_build": "NODE_ENV=production vue-cli-service build",
    "build-bundle": "vue-cli-service --mode production build --target lib --name vue-swipe-actions ./src/components/index.js",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages-deploy"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ]
  },
  "author": {
    "name": "eCollect Sofia Tech Team"
  },
  "keywords": [
    "vuejs",
    "vue-swipe-actions",
    "vue",
    "swipe",
    "list"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eCollect/vue-swipe-actions"
  },
  "dependencies": {
    "tinygesture": "1.1.4"
  },
  "main": "./dist/vue-swipe-actions.common.js",
  "devDependencies": {
    "@vue/cli": "5.0.8",
    "@vue/cli-plugin-babel": "5.0.8",
    "@vue/cli-plugin-eslint": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/cli-service-global": "4.5.19",
    "@vue/compiler-sfc": "3.4.21",
    "eslint": "7.32.0",
    "eslint-plugin-vue": "9.18.1",
    "gh-pages-deploy": "0.5.0",
    "node-sass": "9.0.0",
    "sass-loader": "10.5.2",
    "vue": "3.4.21"
  },
  "peerDependencies": {
    "vue": ">= 3.4.x"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "license": "MIT",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/vue-swipe-actions.common.js",
    "dist/vue-swipe-actions.css"
  ]
}
